    "n = input()\n",
    "len_n = len(n)\n",
    "n=int(n)\n",
    "copy_n=n\n",
    "result = 0\n",
    "i = len_n\n",
    "while(n!=0):\n",
    "    digit = n%10\n",
    "    result=result+pow(digit,i)\n",
    "    n=int(n/10)\n",
    "    i = i - 1\n",
    "if(result==copy_n):\n",
    "    print(\"Disarium Number!\")\n",
    "else:\n",
    "    print(\"Not an Disarium Number!\")"
